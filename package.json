{
  "name": "resume",
  "description": "My resume",
  "version": "1.0.0",
  "author": "Luke Karrys <luke@lukekarrys.com>",
  "dependencies": {
    "cpr": "^2.0.2",
    "html-minifier": "^3.5.1",
    "jsonresume-theme-stackoverflow": "1.0.4",
    "less": "^2.7.2",
    "resume-cli": "^0.4.19",
    "rimraf": "^2.6.1",
    "sync-exec": "^0.6.2"
  },
  "devDependencies": {
    "git-validate": "^2.2.2",
    "standard": "^10.0.2",
    "surge": "^0.19.0"
  },
  "engines": {
    "node": "^8.8.1"
  },
  "keywords": [
    "resume"
  ],
  "license": "MIT",
  "main": "index.js",
  "pre-commit": [
    "test",
    "lint"
  ],
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production resume serve --theme stackoverflow --dir build/ --silent --port 9000 & PID=$!; sleep 1; curl -s http://localhost:9000 > /dev/null; kill -INT $PID;",
    "codeship-deploy": "npm run deploy",
    "codeship-setup": "npm install && npm run build",
    "codeship-test": "npm run lint && npm run test",
    "deploy": "surge --project build/ --domain resume.lukekarrys.com",
    "lint": "standard",
    "postbuild": "node build && cpr build/index.html build/404.html",
    "prebuild": "rimraf build/*.html",
    "start": "resume serve --theme stackoverflow --dir build/",
    "test": "resume test"
  }
}
