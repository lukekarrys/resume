{
  "name": "resume",
  "description": "My resume",
  "version": "1.0.0",
  "author": "Luke Karrys <luke@lukekarrys.com>",
  "dependencies": {
    "cpr": "^2.0.0",
    "jsonresume-theme-stackoverflow": "^1.0.4",
    "less": "^2.7.1",
    "less-plugin-clean-css": "^1.5.1",
    "mkdirp": "^0.5.1",
    "resume-cli": "^0.4.14",
    "rimraf": "^2.5.4",
    "sync-exec": "^0.6.2"
  },
  "devDependencies": {
    "git-validate": "^2.2.2",
    "standard": "^8.3.0",
    "surge": "^0.18.0"
  },
  "engines": {
    "node": "6.0.0"
  },
  "keywords": [
    "resume"
  ],
  "license": "MIT",
  "main": "index.js",
  "pre-commit": [
    "test",
    "lint"
  ],
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production resume serve --theme stackoverflow --dir build/ --silent --port 9000 & PID=$!; echo $PID; sleep 1; curl -s http://localhost:9000 > /dev/null; kill -INT $PID;",
    "deploy": "surge --project build/ --domain resume.lukekarrys.com",
    "lint": "standard",
    "postbuild": "cpr build/index.html build/404.html",
    "prebuild": "rimraf build && mkdirp build",
    "start": "resume serve --theme stackoverflow --dir build/",
    "test": "resume test"
  }
}
