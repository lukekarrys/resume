{
  "name": "resume",
  "description": "My resume",
  "version": "1.0.0",
  "author": "Luke Karrys <luke@lukekarrys.com>",
  "dependencies": {
    "cpr": "^2.0.2",
    "html-minifier": "^3.5.1",
    "jsonresume-theme-stackoverflow": "1.0.4",
    "less": "^2.7.2",
    "resume-cli": "^1.1.2",
    "rimraf": "^2.6.1",
    "sync-exec": "^0.6.2"
  },
  "devDependencies": {
    "git-directory-deploy": "^1.5.1",
    "git-validate": "^2.2.2",
    "json": "^9.0.6",
    "prettier": "^1.15.2"
  },
  "engines": {
    "node": "^8.8.1"
  },
  "keywords": [
    "resume"
  ],
  "license": "MIT",
  "main": "index.js",
  "pre-commit": [
    "test",
    "lint"
  ],
  "private": true,
  "repository": {
    "url": "git@github.com:lukekarrys/resume.git"
  },
  "scripts": {
    "build": "NODE_ENV=production npm start -- --silent --port 9000 & PID=$!; npm run capture; kill -INT $PID;",
    "capture": "sleep 1; curl -s http://localhost:9000 > /dev/null",
    "codeship-deploy": "npm run deploy",
    "codeship-setup": "npm install && npm run build",
    "codeship-test": "npm run lint && npm run test",
    "deploy": "git-directory-deploy --directory build --username lukekarrys --email lukekarrys@gmail.com --repo `cat package.json | json repository.url` --message='--skip-ci'",
    "lint": "prettier --write '**/*.{js,md,css,json}'",
    "postbuild": "node build && cpr build/index.html build/404.html && cpr assets build",
    "prebuild": "rimraf build",
    "start": "resume serve --theme stackoverflow --dir build/",
    "test": "resume test"
  }
}
