  "dependencies": {},
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "autoprefixer": "^9.7.5",
    "cssnano": "^4.1.10",
    "delete": "^1.1.0",
    "git-validate": "^2.2.2",
    "html-minifier": "^4.0.0",
    "marked": "^0.8.2",
    "micro": "^9.3.4",
    "mkdirp": "^1.0.3",
    "playwright": "^0.12.1",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.0",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.2",
    "serve-handler": "^6.1.2",
    "tailwindcss": "^1.2.0"
  },
  "keywords": [
    "resume"
  ],
  "license": "MIT",
  "main": "index.js",
  "pre-commit": [
    "lint:ci"
  ],
  "prettier": {
    "semi": false
  },
  "private": true,
  "repository": {
    "url": "git@github.com:lukekarrys/resume.git"
  },
  "scripts": {
    "build": "npm run build:markdown && npm run build:css && npm run build:pdf",
    "build:css": "postcss src/styles.css -o build/styles.css",
    "build:markdown": "node build.js",
    "build:pdf": "node pdf.js",
    "lint": "prettier --write '**/*.{js,md,css,json}'",
    "lint:ci": "npm run lint -- --list-different",
    "start": "nodemon --exec 'npm run build' -e 'js,md,html,css' --ignore build"
  }
}
