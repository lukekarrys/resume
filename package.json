{
  "name": "resume",
  "description": "My resume",
  "version": "1.0.0",
  "author": "Luke Karrys <luke@lukekarrys.com>",
  "dependencies": {
    "cpr": "^3.0.1",
    "html-minifier": "^3.5.1",
    "jsonresume-theme-stackoverflow": "1.0.4",
    "less": "^3.8.1",
    "lodash": "^4.17.11",
    "resume-cli": "^1.1.2",
    "rimraf": "^2.6.1",
    "sync-exec": "^0.6.2"
  },
  "devDependencies": {
    "git-validate": "^2.2.2",
    "json": "^9.0.6",
    "prettier": "^1.15.2"
  },
  "engines": {
    "node": "^8.8.1"
  },
  "keywords": [
    "resume"
  ],
  "license": "MIT",
  "main": "index.js",
  "pre-commit": [
    "test",
    "lint:ci"
  ],
  "private": true,
  "repository": {
    "url": "git@github.com:lukekarrys/resume.git"
  },
  "scripts": {
    "build": "NODE_ENV=production npm start -- --silent --port 9000 & PID=$!; echo $PID; npm run capture; echo $PID; kill -INT $PID;",
    "capture": "sleep 5; curl -s http://localhost:9000 > /dev/null",
    "lint": "prettier --write '**/*.{js,md,css,json}'",
    "lint:ci": "npm run lint -- --list-different",
    "postbuild": "node build && cpr build/index.html build/404.html && cpr assets build",
    "prebuild": "rimraf build",
    "prestart": "node resume.js",
    "start": "resume serve --theme stackoverflow --dir build/",
    "test": "resume test"
  }
}
